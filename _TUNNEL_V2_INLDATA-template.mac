  ! Thickness of footing
  GEO_HDW1_RIGHT_footThk = 0.40

! > Params for GEO_HDW1_type=1 (wings) only:
  ! Left side:
  ! Wing shape:
  !   [0] Triangular
  !   [1] Rectangular
  GEO_HDW1_LEFT_wingShape = 0
  ! Wing thickness
  GEO_HDW1_LEFT_wingThk = 0.40
  ! Wing length
  GEO_HDW1_LEFT_wingLength = 2.5
  ! Chamfer height (vertical length at the end of the wing)
  GEO_HDW1_LEFT_wingChamferHeight = 0.6
  ! Chamfer width (horizontal length at the bottom of the wing)
  GEO_HDW1_LEFT_wingChamferBase = 0.4
  ! Vertical distance from the bottom of the wing to the tunnel's floor slab
  GEO_HDW1_LEFT_wingBottomOffset = 1.15

  ! Right side:
  ! Wing shape:
  !   [0] Triangular
  !   [1] Rectangular
