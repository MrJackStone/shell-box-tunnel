! _TUNNEL_V2_INLDATA-template


!!                                                                                                !!
!! 1.3  Inlets                                                                                    !!
!!                                                                                                !!
! Inlet option:
!   [0] Off (no inlets)
!   [1] On (at both ends)
!   [2] On, first end only
!   [3] On, second end only
GEO_HDW_status = 1

!! 1.3.1  First end                                                                               !!
! Inlet type:
!   [0] Full inlet (walls, beam, feet, floor slab)
!   [1] Wings (walls only, fixed to tunnel external walls)
!   [2] Approach slab & unconnected retention walls
GEO_HDW1_type = 0

! > Params for GEO_HDW1_type=0 or GEO_HDW1_type=2 only:
  ! Thickness of floor slab
  GEO_HDW1_slabThk = 0.20
  ! Thickness of the slab's stiffened edges (set to zero to repeat slab thickness)
  GEO_HDW1_slabStfThk = 0.20
  ! Width of the slab's stiffened edges (measured from the retaining wall plane)
  GEO_HDW1_slabStfWidth = 1.0

  ! Width of front beam (below floor slab)
  GEO_HDW1_beamWidth = 0.3
  ! Depth of front beam
  GEO_HDW1_beamDepth = 1.2

  ! Left side:
  ! Wall option (for GEO_HDW1_type=2 only):
  !   [0] Off (wall & footing not modelled)
  !   [1] On (model wall & footing)
  GEO_HDW1_LEFT_wallStatus = 1
  ! Thickness of the wall
  GEO_HDW1_LEFT_wallThk = 0.40
  ! Wall length (maximum length, at the bottom)
  GEO_HDW1_LEFT_wallLength = 10
  ! Length of horizontal segment at the top of the wall
  GEO_HDW1_LEFT_wallTopLength = 1.0
  ! Wall opening angle
  GEO_HDW1_LEFT_wallAngle = 15
  ! Width of footing
  GEO_HDW1_LEFT_footWidth = 1.4
