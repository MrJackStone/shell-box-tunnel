    /udoc,arg1,type,off
    /udoc,arg1,typ2,off
    /focus,arg1,1e6,1e6
    *if,arg7,ne,0,then
        clims,MPATHPLOT_VAR_minScale*arg7,MPATHPLOT_VAR_maxScale*arg7
    *endif
    mpathplot,arg8,1


*if,ar13,ne,1,then
    endpng,ar11
*endif

