AR21=arg2+0.01
AR22=arg4+0.02

wincln,arg1

/window,arg1,arg2,arg3,AR20,arg5
/auto,arg1
oplot, arg1, ar10
/user,arg1

*if,ar13,ne,1,then
    go2png,ar12
*endif

    oplot, arg1, ar10
    /device,vector,off
    /gline,arg1,0
    /noerase

    /polygon,4,arg2,arg4,arg3,arg4,arg3,AR20,arg2,AR20
